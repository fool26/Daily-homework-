#include<stdio.h>
#include<math.h>
int pinfang(int n);
int duicheng(int n);
int main(void)
{
	int n;
	scanf("%d",&n);
	if(pinfang(n)==1&&duicheng(n)==1)
	printf("Yes");
	else
	printf("No");
	return 0;
}
int pinfang(int n)
{
	int i,a=0;
	for(i=2;i<=n;i++)
	{
		if(i*i==n)
		a=1;
	}
	if(a==1||n==1||n==0) return 1;
	else return 0;	
}
int duicheng(int n)
{
	int i,p,q,r=0,temp1,temp2;//i是n的位数 
	temp1=n; temp2=n;
	for(i=0;temp1>0;i++,temp1=temp1/10)
	{}//temp1作废了 
	int ar[i];
	for(p=0;p<i;p++)
	{
		ar[p]=temp2%10;
		temp2=temp2/10;
	}
	for(q=0;q<i/2;q++)
	{
		if(ar[q]!=ar[i-1-q])
		break;
	}
	
	if(q==i/2)
	return 1;
	else
	return 0;
	
	
}
