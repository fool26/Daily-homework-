#include<stdio.h>
#include<math.h>
int isPrime(int n);
int isSuper(int n);
int main(void)
{
	int n,i=0,p;
	printf("Please input an interger:\n");
	scanf("%d",&p);
	for(n=pow(10,p-1);n<=pow(10,p)-1;n++)
	{
		if(isSuper(n))
	{	i++;
		if(i%4==1)
		printf("%d",n);
		if(i%4==2||i%4==3)
		printf("\t%d",n);
		if(i%4==0)
		printf("\t%d\n",n);
	}
	}
	return 0;
	
	return 0;
	
}
int isPrime(int n)
{
	int i;
	if(n>2){
	for(i=2;n%i!=0;i++)
	{}
	if(i==n)
	return 1;
	else
	return 0;}
	if(n==2)
	return 1;
	if(n==1)
	return 0;
}
int isSuper(int n)
{
	if(n>=10)
	{
	if(isPrime(n)&&isSuper(n/10))
	return 1;
	}
	else if(n>0&&n<=9)
	{if(isPrime(n))
	return 1;
	}

	
}









