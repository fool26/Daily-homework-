#include<stdio.h>
int pow(int n);
int reverse(int n);
int length1(int n);
int main(void)
{
	int n;
	scanf("%d",&n);

	printf("%d",reverse(n));
	return 0;
} 
int reverse(int n)
{	
	if(n<=9)
	return n;
	else
	return n/(pow(length1(n)-1))+reverse(n%pow(length1(n)-1))*10;
}
int length1(int n)
{
	int i;
	for(i=0;n>0;i++,n=n/10)
	{}
	return i;
}
int pow(int n)
{
	int i,result=1;
	for(i=0;i<n;i++)
	{
		result=result*10;
	}
	return result;
}
