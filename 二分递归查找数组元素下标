#include<stdio.h>
int RecurBinarySearch(int *ar,int first, int last,int key);
int main(void)
{
	int n,key;
	scanf("%d",&n);
	int i=0,ar[n];
	for(i=0;i<n;i++)
	{
		scanf("%d",&ar[i]);
	}
	scanf("%d",&key);
	printf("%d",RecurBinarySearch(ar,0,n-1,key));
	return 0;
} 
int RecurBinarySearch(int *ar,int first,int last,int key)
{
	int mid=(first+last)/2;
	if(ar[mid]==key)
	return mid;
	else if(mid>last||mid<first)
	return -1;
	else if(ar[mid]<key)
	{
		first=mid+1;
		RecurBinarySearch(ar,first,last,key);
	}
	else if(ar[mid]>key)
	{
		last=mid-1;
		RecurBinarySearch(ar,first,last,key);
	}
	
}
